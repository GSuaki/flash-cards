{
  "name": "flash-cards",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test istanbul cover _mocha -- test/**/*.spec.js --exit --require babel-polyfill --compilers js:babel-register --inspect",
    "test:unit": "NODE_ENV=test ./node_modules/.bin/mocha 'test/**/*.spec.js' --exit --require babel-polyfill --compilers js:babel-register --inspect",
    "start": "npm run start-prod",
    "start-prod": "NODE_ENV=production node index.js",
    "start-dev": "export NODE_ENV=development; nodemon --exec babel-node ./index.js",
    "build": "babel app --out-dir build",
    "db-migrate-dev": "export NODE_ENV=development; ./node_modules/.bin/sequelize db:migrate --env development",
    "db-migrate-undo-dev": "./node_modules/.bin/sequelize db:migrate:undo:all --env development",
    "db-migrate": "./node_modules/.bin/sequelize db:migrate --env production",
    "db-migrate-undo": "./node_modules/.bin/sequelize db:migrate:undo:all --env production",
    "db-migrate-stg": "./node_modules/.bin/sequelize db:migrate --env development",
    "db-migrate-stg-undo": "./node_modules/.bin/sequelize db:migrate:undo:all --env development",
    "debug": "export NODE_ENV=development; nodemon --exec babel-node --inspect ./index.js"
  },
  "author": "gsuaki",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.4.5",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-validation": "^1.0.0",
    "ip": "^1.1.3",
    "joi": "^8.0.5",
    "log4js": "^2.10.0",
    "moment": "^2.14.1",
    "moxios": "^0.4.0",
    "mysql2": "^1.5.3",
    "nconf": "^0.8.4",
    "ramda": "^0.25.0",
    "sequelize": "^4.37.10",
    "sequelize-cli": "4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.7.4",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "nodemon": "^1.12.5",
    "chai": "^3.5.0",
    "database-cleaner": "^1.1.0",
    "eslint": "^3.1.1",
    "mocha": "^2.5.3",
    "sinon": "6.0.1",
    "supertest": "^1.2.0",
    "umzug": "^1.11.0",
    "istanbul": "^0.4.5"
  }
}
